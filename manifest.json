{
  "manifest_version": 3,
  "name": "Clipboard History",
  "version": "1.0",
  "description": "Track and manage your clipboard history.",
  "permissions": ["storage", "scripting", "activeTab", "clipboardRead", "clipboardWrite"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
